{% extends '::base.html.twig' %}

{% block body -%}
    <h1>SoftwareLicense list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Softwarelicensetype</th>
                <th>Seats</th>
                <th>Licensekey</th>
                <th>Remark</th>
                <th>Upgradedfrom</th>
                <th>Upgradedto</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('software_licenses_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.softwareLicenseType }}</td>
                <td>{{ entity.seats }}</td>
                <td>{{ entity.licenseKey }}</td>
                <td>{{ entity.remark }}</td>
                <td>{{ entity.upgradedFrom }}</td>
                <td>{{ entity.upgradedTo }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('software_licenses_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('software_licenses_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('software_licenses_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
