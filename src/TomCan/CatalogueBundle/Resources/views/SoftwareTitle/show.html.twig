{% extends 'CatalogueBundle::mybase.html.twig' %}

{% block body -%}{{ parent() }}
    <h1>SoftwareTitle</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Version</th>
                <td>{{ entity.version }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
            <tr>
                <th>Licensetype</th>
                <td>{{ entity.licenseType }}</td>
            </tr>
            <tr>
                <th>Remark</th>
                <td>{{ entity.remark }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('software') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('software_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
